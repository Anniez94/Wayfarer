{
  "name": "wayfarer",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking server.",
  "main": "server/app.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "pg": "^7.11.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --require @babel/register --recursive ./server/test --exit ",
    "build": "babel server --out-dir dist --copy-files ",
    "start": "node dist/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "exec-db-dev": "npm run dev-migration",
    "dev-migration": " babel-node ./server/models/db/migrations.js",
    "migration": "node ./dist/models/db/migrations.js",
    "start:dev": "nodemon --exec babel-node server/app.js",
    "clean": "rm -rf dist && mkdir dist",
    "heroku-postbuild": "npm run clean && npm run build && npm run migration"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "Chiedu Mokwunye",
  "license": "ISC"
}
